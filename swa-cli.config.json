{
    "configurations": {
         "static": {
              "appLocation": "./dist/wwwroot",
              "apiLocation": "./api"
         },
         "run-all": {
              "appDevserverUrl": "http://localhost:7001",
              "apiLocation": "./Api",
              "run": "dotnet watch run --project ./Client --launch-profile https"
         },
         "debug": {
              "appDevserverUrl": "http://localhost:7001"
         },
         "deploy-config": {
                "appLocation": "./dist/client/wwwroot",
                "apiLocation": "./dist/api",
                "outputLocation": ".",
                "apiLanguage": "dotnetisolated",
                "apiVersion": "9.0",
                "apiBuildCommand": "dotnet publish -c Release"
         }  
    }  

}
